{
  "presets": [
    ["env", {
      "targets": {
        "node": "current"
      },
      "modules": false
    }],
    "react",
    "stage-2"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    ["transform-react-jsx"]
  ],
  "env": {
    "test": {
      "presets": [
        ["env", {
          "targets": {
            "node": "current"
          },
          "modules": false
        }],
        "react",
        "stage-2"
      ],
      "plugins": [
        "transform-object-rest-spread"
      ]
    }
  }
}
