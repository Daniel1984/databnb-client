{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "modules": false
    }]
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    ["transform-react-jsx", { "pragma":"h" }]
  ],
  "env": {
    "test": {
      "presets": [
        ["env", {
          "targets": {
            "node": "current"
          }
        }]
      ],
      "plugins": [
        "transform-object-rest-spread"
      ]
    }
  }
}
