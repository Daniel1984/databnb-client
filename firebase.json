{
  "hosting": {
    "public": "./public",
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }],
    "headers": [{
      "source" : "**/*.html",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "private, max-age=0, no-cache"
      }]
    }]
  }
}
